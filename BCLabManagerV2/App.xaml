<prism:PrismApplication x:Class="BCLabManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                        xmlns:prism="http://prismlibrary.com/"
             >
    <Application.Resources>
        <SolidColorBrush x:Key="WaitingBrush" Color="#FFF2D2"></SolidColorBrush>
        <SolidColorBrush x:Key="ExecutingBrush" Color="#3F88C5"></SolidColorBrush>
        <SolidColorBrush x:Key="CompletedBrush" Color="#56A3A6"></SolidColorBrush>
        <SolidColorBrush x:Key="InvalidBrush" Color="#D00000"></SolidColorBrush>
        <SolidColorBrush x:Key="AbandonedBrush" Color="#595F66"></SolidColorBrush>
        <SolidColorBrush x:Key="UsingBrush" Color="#D00000"></SolidColorBrush>
        <SolidColorBrush x:Key="IdleBrush" Color="#FFF2D2"></SolidColorBrush>
    </Application.Resources>
</prism:PrismApplication>
