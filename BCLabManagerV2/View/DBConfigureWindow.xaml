<Window x:Class="BCLabManager.View.DBConfigureWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BCLabManager.View"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="DBConfigureWindow" Height="270" Width="470">
    <Grid>
        <Label Content="Please setup database location" HorizontalAlignment="Left" Margin="36,30,0,0" VerticalAlignment="Top" Height="27" Width="237"/>
        <Button Content="OK" IsDefault="True" HorizontalAlignment="Left" Margin="353,187,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <Button Content="Browse" HorizontalAlignment="Left" Margin="376,110,0,0" VerticalAlignment="Top" Width="52" Click="Button_Click"/>
        <TextBox x:Name="FilePath" HorizontalAlignment="Left" Height="19" Margin="36,110,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="335"/>
    </Grid>
</Window>
